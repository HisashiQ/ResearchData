    ]
  },
  {
    "user": "U055GGAHS",
    "text": "that operation would evaluate true",
    "edited": {
      "user": "U055GGAHS",
      "ts": "1438003366.000000"
    },
    "ts": "1438003352.000648",
    "id": "27de5296",
